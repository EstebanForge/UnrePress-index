{
  "schema_version": 1,
  "type": "theme", // For themes, fixed to "theme"
  "name": "", // Theme name
  "slug": "", // Theme slug: sanitize_title_with_dashes(name)
  "author": "", // Author name
  "author_profile": "", // Author profile URL
  "sections": {
    "description": "", // Theme description
    "installation": "", // Theme installation instructions
    "changelog": "" // Theme changelog
  },
  "banners": {
    "low": "", // Low resolution banner URL: 772x250
    "high": "" // High resolution banner URL: 1544x500
  },
  "icons": {
    "low": "", // Low resolution icon URL: 256x256
    "high": "" // High resolution icon URL: 1024x1024
  },
  "homepage": "", // Theme homepage URL
  "readme_md": "", // Theme README full URL, in Markdown format
  "readme_txt": "", // Theme README full URL, in plain text WP format
  "unrepress-meta": {
    "wp-meta": false, // True if the repository contains a root .wp-meta/ folder with extra data for UnrePress
    "free": true, // Free theme. False if it's paid only
    "paid_features": true, // Has paid features, beside the free ones
    "date_added": "2024-12-24", // Date when the theme was added to UnrePress Index
    "repository": "", // Theme repository on: GitHub, BitBucket, GitLab or Gitea
    "tags": "", // Theme tags full URL
    "update_from": "tags", // For now, fixed to "tags"
    "abandoned": false, // If the theme is abandoned
    "taken-down": false, // If the theme is taken down
    "vulnerable": false // If the theme has any known vulnerabilities
  }
}
