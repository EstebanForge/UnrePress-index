{
  "schema_version": 1,
  "type": "plugin", // For plugins, fixed to "plugin"
  "name": "", // Plugin name
  "slug": "", // Plugin slug: sanitize_title_with_dashes(name)
  "author": "", // Author name
  "author_url": "", // Author profile URL
  "sections": {
    "description": "", // Plugin description
    "installation": "", // Installation instructions
    "changelog": "" // Changelog URL
  },
  "banners": {
    "low": "", // Low resolution banner URL: 772x250
    "high": "" // High resolution banner URL: 1544x500
  },
  "icons": {
    "low": "", // Low resolution icon URL: 256x256
    "high": "" // High resolution icon URL: 1024x1024
  },
  "homepage": "", // Plugin homepage URL
  "readme_md": "", // Plugin README full URL, in Markdown format
  "readme_txt": "", // Plugin README full URL, in plain text WP format
  "unrepress-meta": {
    "wp-meta": false, // True if the repository contains a root .wp-meta/ folder with extra data for UnrePress
    "free": true, // Free plugin. False if it's paid only
    "paid_features": false, // Has paid features, beside the free ones
    "date_added": "2024-12-23", // Date when the plugin was added to UnrePress Index
    "repository": "", // Plugin repository on: GitHub, BitBucket, GitLab or Gitea
    "tags": "", // Plugin tags full URL
    "update_from": "tags", // For now, fixed to "tags"
    "abandoned": false, // If the plugin is abandoned
    "taken-down": false, // If the plugin is taken down by the author
    "vulnerable": false // If the plugin has any known vulnerabilities
  }
}
